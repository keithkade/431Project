<p id="notice"><%= notice %></p>

<h1>Listing Course User Relations</h1>

<table>
  <thead>
    <tr>
      <th>Course</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @course_user_relations.each do |course_user_relation| %>
      <tr>
        <td><%= course_user_relation.course %></td>
        <td><%= course_user_relation.user %></td>
        <td><%= link_to 'Show', course_user_relation %></td>
        <td><%= link_to 'Edit', edit_course_user_relation_path(course_user_relation) %></td>
        <td><%= link_to 'Destroy', course_user_relation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

