<% content_for :title do %>List of Problems<% end %>

<% content_for :options do %>
<% end %>

<table class="problems-list-table table">
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @problems.each do |problem| %>
      <tr>
        <td><%= problem.title %></td>
        <td><%= link_to 'View', problem, :class => 'btn btn-default' %></td>
      </tr>
    <% end %>
  </tbody>
</table>