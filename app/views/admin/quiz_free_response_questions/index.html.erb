<p id="notice"><%= notice %></p>

<h1>Listing Quiz Free Response Questions</h1>

<table>
  <thead>
    <tr>
      <th>Correct answer</th>
      <th>Question</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @quiz_free_response_questions.each do |quiz_free_response_question| %>
      <tr>
        <td><%= quiz_free_response_question.correct_answer %></td>
        <td><%= quiz_free_response_question.question %></td>
        <td><%= link_to 'Show', quiz_free_response_question %></td>
        <td><%= link_to 'Edit', edit_quiz_free_response_question_path(quiz_free_response_question) %></td>
        <td><%= link_to 'Destroy', quiz_free_response_question, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Quiz free response question', new_quiz_free_response_question_path %>
