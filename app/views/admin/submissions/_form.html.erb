<%= form_for [:admin, @submission] do |f| %>
  <% if @submission.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@submission.errors.count, "error") %> prohibited this submission from being saved:</h2>

      <ul>
      <% @submission.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :studentid %><br>
    <%= f.number_field :studentid %>
  </div>
  <div class="field">
    <%= f.label :problemID %><br>
    <%= f.number_field :problemid %>
  </div>
  <div class="field">
    <%= f.label :code %><br>
    <%= f.text_area :code %>
  </div>
  <div class="field">
    <%= f.label :timeOnPage %><br>
    <%= f.datetime_select :timeOnPage %>
  </div>
  <div class="field">
    <%= f.label :timeSubmitted %><br>
    <%= f.datetime_select :timeSubmitted %>
  </div>
  <div class="field">
    <%= f.label :result %><br>
    <%= f.text_area :result %>
  </div>
  <div class="field">
    <%= f.label :status %><br>
    <%= f.check_box :status %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
