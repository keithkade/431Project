<h1>Listing Submissions</h1>

<table>
  <thead>
    <tr>
      <th>Student ID</th>
      <th>Problem ID</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @submissions.each do |submission| %>
      <tr>
        <td><%= submission.studentID %></td>
        <td><%= submission.problemID %></td>
        <td><%= link_to 'Show', submission %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>

